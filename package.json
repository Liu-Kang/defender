{
  "name": "defender2",
  "version": "1.0.0",
  "description": "it's not a avenger but a defender!",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-scripts": "^2.11.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "conventional-changelog-cli": "^2.0.23",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "husky": "^3.0.5"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-defender2",
    "stop": "egg-scripts stop --title=egg-server-defender2",
